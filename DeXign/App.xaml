             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Resources/Colors.xaml"/>
                <ResourceDictionary Source="Themes/Resources/Paths.xaml"/>
                
                <ResourceDictionary Source="Themes/Resources/ToolBoxResources.xaml"/>
                
