<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="clr-namespace:DeXign.Core.Designer;assembly=DeXign.Core"
                    xmlns:c="clr-namespace:DeXign.Core.Controls;assembly=DeXign.Core"
                    xmlns:l="clr-namespace:DeXign.Core.Logic;assembly=DeXign.Core">

    <Style x:Key="ToolBoxItemIconPathStyle" TargetType="{x:Type Path}">
        <Setter Property="Fill" Value="Black"/>
        <Setter Property="Margin" Value="8"/>
        <Setter Property="Stretch" Value="Uniform"/>
    </Style>

    <!-- #region Layouts -->
    <d:DesignerResource x:Key="{x:Type c:PGridLayout}">
        <d:DesignerResource.ToolTip>
            <ToolTip>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <Image Source="/DeXign;component/Resources/Images/GridLayoutTooltip.png" Width="66" Height="128" Margin="4,0" Stretch="Uniform" VerticalAlignment="Center" HorizontalAlignment="Center"/>

                    <TextBlock Grid.Column="1" Text="그리드에 대한 설명 주저리 주저리" VerticalAlignment="Center" Margin="8,0"/>
                </Grid>
            </ToolTip>
        </d:DesignerResource.ToolTip>
        <Image Source="Icons/IconGridLayout.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PScrollView}">
        <d:DesignerResource.ToolTip>
            <ToolTip>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <Image Source="/DeXign;component/Resources/Images/ScrollViewTooltip.png" Width="66" Height="128" Margin="4,0" Stretch="Uniform" VerticalAlignment="Center" HorizontalAlignment="Center"/>

                    <TextBlock Grid.Column="1" Text="스크롤에 대한 설명 주저리 주저리" VerticalAlignment="Center" Margin="8,0"/>
                </Grid>
            </ToolTip>
        </d:DesignerResource.ToolTip>
        <Image Source="Icons/IconScrollViewer.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PStackLayout}">
        <d:DesignerResource.ToolTip>
            <ToolTip>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <Image Source="/DeXign;component/Resources/Images/StackLayoutTooltip.png" Width="66" Height="128" Margin="4,0" Stretch="Uniform" VerticalAlignment="Center" HorizontalAlignment="Center"/>

                    <TextBlock Grid.Column="1" Text="스택에 대한 설명 주저리 주저리" VerticalAlignment="Center" Margin="8,0"/>
                </Grid>
            </ToolTip>
        </d:DesignerResource.ToolTip>
        <Image Source="Icons/IconStackLayout.png"/>
    </d:DesignerResource>
    <!-- #endregion -->

    <!-- #region Controls -->
    <d:DesignerResource x:Key="{x:Type c:PLabel}">
        <Image Source="Icons/IconLabel.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PBoxView}">
        <Image Source="Icons/IconBoxView.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PButton}">
        <Image Source="Icons/IconButton.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PImage}">
        <Image Source="Icons/IconImage.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PProgressBar}">
        <Image Source="Icons/IconProgress.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PWebView}">
        <Image Source="Icons/IconWebView.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PSlider}">
        <Image Source="Icons/IconSlider.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type c:PContentPage}">
        <Image Source="Icons/IconScreen.png"/>
    </d:DesignerResource>
    <!-- #endregion -->

    <d:DesignerResource x:Key="{x:Type l:PGetter}">
        <Image Source="Icons/IconGetter.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type l:PSetter}">
        <Image Source="Icons/IconSetter.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type l:PBranch}">
        <Image Source="Icons/IconBranch.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type l:PTrigger}">
        <Image Source="Icons/IconTrigger.png"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type l:PFunction}">
        <Image Source="Icons/IconFunction.png"/>
    </d:DesignerResource>
</ResourceDictionary>