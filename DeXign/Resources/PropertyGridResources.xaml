<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="clr-namespace:DeXign.Core.Designer;assembly=DeXign.Core">

    <d:DesignerResource x:Key="UnknownPropertyTemplate">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Unknown - " Foreground="Brown"/>
            <TextBlock Text="{Binding Metadata.Element.PropertyType.Name}"/>
        </StackPanel>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type sys:String}" ToolTip="{Binding Metadata.Attribute.DisplayName}">
        <TextBox VerticalAlignment="Center" Background="Transparent" BorderThickness="0" Foreground="#535353"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type sys:Int64}">
        <TextBlock Text="int"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type sys:Double}">
        <TextBlock Text="double"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type sys:Enum}">
        <TextBlock Text="Enum"/>
    </d:DesignerResource>

    <d:DesignerResource x:Key="{x:Type Brush}">
        <TextBlock Text="Brush"/>
    </d:DesignerResource>
</ResourceDictionary>